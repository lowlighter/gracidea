{
  "name": "gracidea",
  "version": "1.0.0",
  "description": "Pok√©mon world map project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/app.js",
    "start-dev": "node server/app.js --dev 1",
    "build": "npx run-s build-transpile build-bundle",
    "build-bundle": "npx rollup --input client/js/build/gracidea.js --file client/js/gracidea.min.js --format iife",
    "build-transpile": "npx babel client/js/src --out-dir client/js/build/ --plugins \"@babel/proposal-class-properties\" --presets minify --no-comments",
    "jsprite-overworld": "node utils/tileset-json.js --margin --spacing 2 --destination maps/overworld/tileset.textures.json",
    "usprite-overworld": "node utils/tileset.js --margin 2 --spacing 2 --extrusion 1 --source maps/overworld/tileset.textures.raw.png"
  },
  "author": "lowlighter",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "minimist": "^1.2.5",
    "pixi-viewport": "^4.10.0",
    "pixi.js": "^5.2.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-preset-minify": "^0.4.3",
    "colors": "^1.4.0",
    "jimp": "^0.10.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.6.0",
    "tile-extruder": "^2.0.5"
  }
}
